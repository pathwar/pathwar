{
  "name": "pathwar.land",
  "private": true,
  "description": "The Pathwar Project",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "env-cmd --file ./.env.test jest -c ./tests/jest.config.js",
    "test:watch": "env-cmd --file ./.env.test jest -c ./tests/jest.config.js --watch",
    "docker-integration": "env-cmd --file ./.env.docker-integration jest",
    "integration": "env-cmd --file ./.env.integration jest",
    "start:local-api": "env-cmd --file ./.env.integration npm run develop"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@loadable/component": "^5.10.2",
    "@sentry/browser": "^5.17.0",
    "axios": "^0.19.0",
    "emotion-theming": "^10.0.27",
    "env-cmd": "^10.0.1",
    "formik": "^1.5.8",
    "gatsby": "^2.18.10",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-emotion": "^4.3.2",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sass": "^2.3.10",
    "gatsby-plugin-sharp": "2.2.14",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-sharp": "^2.2.4",
    "js-cookie": "^2.2.0",
    "keycloak-js": "^9.0.0",
    "node-sass": "^4.14.1",
    "object-assign": "4.1.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "ramda": "^0.27.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.1.0",
    "react-responsive-modal": "^5.0.2",
    "react-toastify": "^5.4.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "tabler-react": "^1.30.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.20",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
